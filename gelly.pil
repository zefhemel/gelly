import "ast.pil"
import "parser.pil"
//import "evaluator.pil"
//import "pretty.pil"

import gelly


void main(Array<String> args) {
  String input = null;
  println("Welcome to gelly, type :q to quit.");
  while(input != ":q") {
    print("> ");
    input = readln();
    if(input != ":q") {
      println(new Parser(input).acceptExp().toIndentedString(0));
    }
  }
}
